{
  "expo": {
    "name": "RuneoDrive",
    "slug": "runeodrive",
    "version": "2025.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "owner": "etml-inf-dev",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "ch.mosiby.runeo",
      "buildNumber": "2025.0.0"
    },
    "plugins": [
      [
        "expo-build-properties",
        {
          "android": {
            "usesCleartextTraffic": true
          }
        }
      ],
      "expo-font"
    ],
    "android": {
      "permissions": ["INTERNET", "ACCESS_NETWORK_STATE"],
      "adaptiveIcon": {
        "foregroundImage": "./assets/icon.png",
        "backgroundColor": "#FFFFFF"
      },
      "package": "ch.mosiby.runeo",
      "versionCode": 202500
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "df624953-600d-44b4-8de0-553244a93199"
      }
    }
  }
}
